@model IEnumerable<AspApp_VenteVetements.Models.VetementCommande>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Commande.AdresseLivraison)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Vetement.nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quantite)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Commande.AdresseLivraison)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vetement.nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quantite)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
